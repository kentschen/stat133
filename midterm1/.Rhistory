gestByAge <- function(age.cutoff, ages, gestation){
if(age.cutoff < min(ages, na.rm=T)) stop("Age cutoff too low")
subset.idcs <- ages <- age.cutoff
subset.avg <- mean(getation[subset.idcs], na.rm = T)
return(subset.avg)
